// B R E A D C R U M B


.m-breadcrumb
  padding 0
  margin-bottom 3.125rem //50px
  background-color transparent
  li
    a
      color $base-dark
  @media (max-width:$breakpoint-sm)
    font-size $font-size-small

.breadcrumb-item+.breadcrumb-item::before
  color $base-dark
  content ">"

.breadcrumb-item.active
  color $base-dark
